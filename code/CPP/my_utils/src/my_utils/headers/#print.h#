#include <iostream>
#include <initializer_list>

template<typename Type>
void println( Type args )
{
  std::cout << args << std::endl;
}

template<typename Type>
void println( std::initializer_list<Type> args )
{
  std::for_each( args.begin(), args.end(),
		 []( Type t ){ std::cout << T; });
    std::cout << std::endl;
}
