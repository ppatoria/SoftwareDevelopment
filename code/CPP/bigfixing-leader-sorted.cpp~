#include <vector>
#include <algorithm>
int solution(vector<int>& A) {
    int n = A.size();
    vector<int> L;
    L.push_back(-1);
    for (int i = 0; i < n; i++) {
        L.push_back(A[i]);
    }
    int count = 0;
    int pos = n / 2;
    int candidate = L[pos];
    for (int i = 1; i <= n; i++) {
        if (L[i] == candidate)
            count = count + 1;
    }
    if (count > pos)
        return candidate;
    return (-1);
}
